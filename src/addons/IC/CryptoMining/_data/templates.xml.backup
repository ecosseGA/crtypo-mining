!function() {
	// Real-time conversion calculator
	const amountInput = document.getElementById('amount');
	const estimatedReceive = document.getElementById('estimatedReceive');
	const btcPrice = {{$market.current_price}};
	
	if (amountInput &amp;&amp; estimatedReceive) {
		amountInput.addEventListener('input', function() {
			const amount = parseFloat(this.value) || 0;
			const total = amount * btcPrice;
			estimatedReceive.textContent = '$' + total.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ',');
		});
	}
}();
