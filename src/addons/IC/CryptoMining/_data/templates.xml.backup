&lt;script&gt;
// Mining Grid JavaScript
const gridElement = document.getElementById('miningGrid');

// Click handler for grid blocks
if (gridElement) {
	gridElement.addEventListener('click', function(e) {
		const block = e.target.closest('.cryptoGrid-block');
		if (!block || block.classList.contains('is-mined')) return;
		
		const position = parseInt(block.dataset.position);
		const rigId = document.getElementById('selectedRig')?.value;
		const useInsurance = document.getElementById('useInsurance')?.checked || false;
		
		if (!rigId) {
			alert('Please select a mining rig first!');
			return;
		}
		
		// Disable clicking while mining
		block.style.pointerEvents = 'none';
		block.classList.add('is-mining');
		
		// Create and submit form
		const form = document.createElement('form');
		form.method = 'POST';
		form.action = XF.canonicalizeUrl('crypto-grid/mine');
		
		const posField = document.createElement('input');
		posField.type = 'hidden';
		posField.name = 'position';
		posField.value = position;
		form.appendChild(posField);
		
		const rigField = document.createElement('input');
		rigField.type = 'hidden';
		rigField.name = 'rig_id';
		rigField.value = rigId;
		form.appendChild(rigField);
		
		const insField = document.createElement('input');
		insField.type = 'hidden';
		insField.name = 'use_insurance';
		insField.value = useInsurance ? 1 : 0;
		form.appendChild(insField);
		
		// Add CSRF token
		const csrfField = document.createElement('input');
		csrfField.type = 'hidden';
		csrfField.name = '_xfToken';
		csrfField.value = XF.config.csrf;
		form.appendChild(csrfField);
		
		document.body.appendChild(form);
		form.submit();
	});
}

function updateRigDurability(rigId, newDurability) {
	const option = document.querySelector('#selectedRig option[value="' + rigId + '"]');
	if (option) {
		option.dataset.durability = newDurability;
		option.textContent = option.textContent.replace(/\(Durability: [\d.]+%\)/, '(Durability: ' + newDurability.toFixed(1) + '%)');
		
		// If durability too low, disable option
		if (newDurability &lt;= 20) {
			option.disabled = true;
			option.textContent += ' - TOO LOW!';
			// Auto-deselect if this was selected
			if (document.getElementById('selectedRig').value == rigId) {
				document.getElementById('selectedRig').value = '';
			}
		}
	}
}
