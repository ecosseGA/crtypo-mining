!function() {
	// Smooth tab switching without page reload
	const tabs = document.querySelectorAll('.cryptoLeaderboard-tab');
	
	tabs.forEach(tab =&gt; {
		tab.addEventListener('click', function(e) {
			e.preventDefault();
			const url = this.getAttribute('href');
			
			// Smooth scroll to leaderboard
			const leaderboard = document.querySelector('.cryptoLeaderboard-header');
			if (leaderboard) {
				leaderboard.scrollIntoView({ behavior: 'smooth', block: 'start' });
			}
			
			// Small delay then navigate
			setTimeout(() =&gt; {
				window.location.href = url;
			}, 300);
		});
	});
}();
