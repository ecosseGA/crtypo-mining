// === PURCHASE FORM ===
.cryptoPurchaseForm
{
	.cryptoPurchaseForm-actions
	{
		display: flex;
		gap: 12px;
		justify-content: center;
		flex-wrap: wrap;
	}
}

// === BUTTONS ===
.cryptoButton
{
	display: inline-block;
	padding: 10px 20px;
	border-radius: 8px;
	font-size: 14px;
	font-weight: 600;
	text-align: center;
	text-decoration: none;
	transition: all 0.2s ease;
	border: 2px solid transparent;
	cursor: pointer;
	line-height: 1.4;
	
	&amp;.cryptoButton--primary
	{
		background: #3b82f6;
		color: #fff;
		
		&amp;:hover
		{
			background: #2563eb;
			box-shadow: 0 4px 12px rgba(59, 130, 246, 0.3);
		}
	}
	
	&amp;.cryptoButton--secondary
	{
		background: #fff;
		color: #3b82f6;
		border-color: #3b82f6;
		
		&amp;:hover
		{
			background: #eff6ff;
		}
	}
	
	&amp;.cryptoButton--disabled
	{
		background: #e5e7eb;
		color: #9ca3af;
		cursor: not-allowed;
		opacity: 0.6;
		
		&amp;:hover
		{
			background: #e5e7eb;
			box-shadow: none;
		}
	}
	
	&amp;.cryptoButton--large
	{
		padding: 14px 32px;
		font-size: 16px;
	}
}

// === EMPTY STATE ===
.cryptoEmptyState
{
	background: #f9fafb;
	border: 2px dashed #d1d5db;
	border-radius: 12px;
	padding: 48px 24px;
	text-align: center;
	margin: 32px 0;
	
	.cryptoEmptyState-icon
	{
		font-size: 64px;
		margin-bottom: 16px;
	}
	
	.cryptoEmptyState-title
	{
		font-size: 20px;
		font-weight: 700;
		color: #1f2937;
		margin-bottom: 8px;
	}
	
	.cryptoEmptyState-text
	{
		font-size: 14px;
		color: #6b7280;
		margin-bottom: 24px;
	}
}

// === ALERT (for messages) ===
.cryptoAlert
{
	padding: 16px 20px;
	border-radius: 8px;
	margin-bottom: 24px;
	display: flex;
	align-items: center;
	gap: 12px;
	
	.cryptoAlert-icon
	{
		font-size: 20px;
	}
	
	.cryptoAlert-content
	{
		font-size: 14px;
		line-height: 1.5;
		
		a
		{
			text-decoration: underline;
			font-weight: 600;
		}
	}
	
	&amp;.cryptoAlert--info
	{
		background: #eff6ff;
		color: #1e40af;
		border: 1px solid #3b82f6;
	}
}

// === RESPONSIVE ===
@media (max-width: 768px)
{
	.cryptoMiningContainer
	{
		padding: 12px;
	}
	
	.cryptoWalletStats,
	.cryptoRigItem-stats,
	.cryptoShopItem-specs,
	.cryptoSpecsGrid
	{
		grid-template-columns: 1fr 1fr;
	}
	
	.cryptoShopGrid
	{
		grid-template-columns: 1fr;
	}
	
	.cryptoMarketBanner,
	.cryptoBalanceCard
	{
		flex-direction: column;
		text-align: center;
	}
	
	.cryptoRigStat
	{
		display: block;
		margin: 4px 0;
	}
}

@media (max-width: 480px)
{
	.cryptoWalletStats,
	.cryptoRigItem-stats,
	.cryptoShopItem-specs
	{
		grid-template-columns: 1fr;
	}
	
	.cryptoSpecsGrid
	{
		grid-template-columns: repeat(2, 1fr);
	}
	
	.cryptoPurchaseForm-actions
	{
		flex-direction: column;
		
		.cryptoButton
		{
			width: 100%;
		}
	}
}

