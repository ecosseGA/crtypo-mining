// === PRICE BANNER ===
.cryptoPriceBanner
{
	background: linear-gradient(135deg, #f59e0b 0%, #d97706 100%);
	color: #fff;
	padding: 24px;
	border-radius: 12px;
	margin-bottom: 24px;
	text-align: center;
	box-shadow: 0 4px 12px rgba(245, 158, 11, 0.3);
	
