	<template type="public" title="ic_crypto.less" addon_id="IC/CryptoMining" version_id="1090013" version_string="1.9.0.13">
// ========================== UNIFIED COLOR PALETTE ==========================
// Use these colors consistently across ALL pages

// Primary Colors
@crypto-primary: #f7931a;        // Bitcoin orange
@crypto-gold: #ffd700;           // Gold highlights
@crypto-success: #4caf50;        // Green (profits, active)
@crypto-danger: #ef5350;         // Red (losses, danger)
@crypto-warning: #ff9800;        // Orange (warnings)
@crypto-info: #2196f3;           // Blue (info)

// Backgrounds
@crypto-bg-dark: #1a1a2e;        // Dark background
@crypto-bg-darker: #16213e;      // Darker background
@crypto-bg-card: rgba(255, 255, 255, 0.05);  // Card background
@crypto-bg-card-hover: rgba(255, 255, 255, 0.08);  // Card hover

// Text
@crypto-text-primary: #ffffff;
@crypto-text-secondary: rgba(255, 255, 255, 0.7);
@crypto-text-muted: rgba(255, 255, 255, 0.5);

// Borders
@crypto-border: rgba(255, 255, 255, 0.1);
@crypto-border-highlight: rgba(247, 147, 26, 0.3);  // Bitcoin orange border
@crypto-border-gold: rgba(255, 215, 0, 0.3);        // Gold border

// Shadows
@crypto-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);
@crypto-shadow-glow: 0 0 20px rgba(247, 147, 26, 0.3);

// Gradients
@crypto-gradient-dark: linear-gradient(135deg, @crypto-bg-dark 0%, @crypto-bg-darker 100%);
@crypto-gradient-gold: linear-gradient(135deg, @crypto-gold 0%, #ff8c00 100%);
@crypto-gradient-orange: linear-gradient(135deg, @crypto-primary 0%, #e67e22 100%);

// ########## CRYPTO MINING - COMPLETELY CUSTOM STYLES ##########
// Zero XenForo structural dependencies - Pure custom design

// === CONTAINER ===
.cryptoMiningContainer
{
	max-width: 1200px;
	margin: 0 auto;
	padding: 20px;
}

// === MARKET BANNER ===
.cryptoMarketBanner
{
	background: linear-gradient(135deg, #f59e0b 0%, #d97706 100%);
	color: #fff;
	padding: 24px;
	border-radius: 12px;
	margin-bottom: 24px;
	display: flex;
	align-items: center;
	gap: 16px;
	box-shadow: 0 4px 12px rgba(245, 158, 11, 0.3);
	
	.cryptoMarketBanner-icon
	{
		font-size: 32px;
	}
	
	.cryptoMarketBanner-content
	{
		.cryptoMarketBanner-label
		{
			font-size: 12px;
			text-transform: uppercase;
			letter-spacing: 1px;
			opacity: 0.9;
			font-weight: 600;
		}
		
		.cryptoMarketBanner-price
		{
			font-size: 28px;
			font-weight: 700;
			line-height: 1.2;
		}
	}
}

// === WALLET CARD ===
.cryptoWalletCard
{
	background: #fff;
	border: 2px solid #e5e7eb;
	border-radius: 12px;
	padding: 24px;
	margin-bottom: 24px;
	box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
}

.cryptoCardHeader
{
	display: flex;
	align-items: center;
	gap: 12px;
	margin-bottom: 20px;
	
	.cryptoCardHeader-icon
	{
		font-size: 28px;
	}
	
	.cryptoCardHeader-title
	{
		font-size: 22px;
		font-weight: 700;
		color: #1f2937;
	}
}

.cryptoWalletStats
{
	display: grid;
	grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
	gap: 20px;
}

.cryptoStat
{
	.cryptoStat-label
	{
		font-size: 11px;
		text-transform: uppercase;
		letter-spacing: 0.8px;
		color: #6b7280;
		font-weight: 600;
		margin-bottom: 6px;
	}
	
	.cryptoStat-value
	{
		font-size: 24px;
		font-weight: 700;
		color: #1f2937;
		line-height: 1.2;
		margin-bottom: 4px;
		
		&amp;.cryptoStat-value--cash
		{
			color: #16c784; // Green for money
		}
		
		&amp;.cryptoStat-value--btc
		{
			color: @crypto-primary; // Bitcoin orange
		}
	}
	
	.cryptoStat-subtext
	{
		font-size: 12px;
		color: #9ca3af;
	}
}

// === BALANCE CARD (Shop) ===
.cryptoBalanceCard
{
	background: linear-gradient(135deg, #10b981 0%, #059669 100%);
	color: #fff;
	padding: 24px;
	border-radius: 12px;
	margin-bottom: 24px;
	display: flex;
	align-items: center;
	gap: 16px;
	box-shadow: 0 4px 12px rgba(16, 185, 129, 0.3);
	
	.cryptoBalanceCard-icon
	{
		font-size: 32px;
	}
	
	.cryptoBalanceCard-content
	{
		.cryptoBalanceCard-label
		{
			font-size: 12px;
			text-transform: uppercase;
			letter-spacing: 1px;
			opacity: 0.9;
		}
		
		.cryptoBalanceCard-amount
		{
			font-size: 32px;
			font-weight: 700;
			line-height: 1.2;
			margin: 4px 0;
		}
		
		.cryptoBalanceCard-subtext
		{
			font-size: 13px;
			opacity: 0.85;
		}
	}
}

// === SECTION HEADER ===
.cryptoSection
{
	margin-top: 32px;
}

.cryptoSectionHeader
{
	display: flex;
	align-items: center;
	gap: 12px;
	margin-bottom: 16px;
	padding-bottom: 12px;
	border-bottom: 2px solid #e5e7eb;
	
	.cryptoSectionHeader-icon
	{
		font-size: 24px;
	}
	
	.cryptoSectionHeader-title
	{
		font-size: 20px;
		font-weight: 700;
		color: #1f2937;
		flex: 1;
	}
	
	.cryptoSectionHeader-count
	{
		font-size: 14px;
		color: #6b7280;
		background: #f3f4f6;
		padding: 4px 12px;
		border-radius: 12px;
		font-weight: 600;
